{
  "name": "fretr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "netlify-lambda": "^1.6.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "yarn postcss:css && react-scripts start",
    "build": "yarn crossenv:set && npm run postcss:css && react-scripts build",
    "postcss:css": "tailwind build src/css/tailwind.css -o src/css/main.css",
    "crossenv:set": "cross-env NODE_ENV=production",
    "prod": "yarn build; yarn build:lambda",
    "start:lambda": "netlify-lambda serve functions",
    "build:lambda": "netlify-lambda build functions",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.5"
  }
}